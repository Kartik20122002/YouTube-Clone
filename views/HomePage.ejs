     <style>
        /* width */
        ::-webkit-scrollbar {
          width: 10px;
        }
        
        /* Track */
        ::-webkit-scrollbar-track {
          background: #f1f1f17d; 
        }
         
        /* Handle */
        ::-webkit-scrollbar-thumb {
          background: rgba(136, 136, 136, 0.541); 
          border-radius : 10px;
        }
        
        /* Handle on hover */
        ::-webkit-scrollbar-thumb:hover {
          background: #555;
          opacity: 1;
        }

        
        </style>
